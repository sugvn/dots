
separator_block_width=0
separator=""

[time]
command=date +"| %b %d | %I:%M %p |"
interval= 15

[volume]
command=~/.config/i3blocks/volume.sh
interval=once
signal=10


[Mute]
command=~/.config/i3blocks/mute
interval=once
signal=11

[wifi]
command=~/.config/i3blocks/wifi2
interval=5
signal=12

[bluetooth]
command=~/.config/i3blocks/blue
interval=5
signal=13
